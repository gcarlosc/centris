<div class="form-horizontal col-xs-12 col-sm-12 col-md-12 col-lg-12">
  <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
    <%= f.input :originable_id, collection: Warehouse.all, label_method: :fullname, value_method: :id, include_blank: 'Elija almacen...', label: 'Almacen' %>
    <%= f.input :originable_type, input_html: { value: 'Warehouse' }, as: :hidden %>
  </div>

  <div class="col-xs-12 col-sm-12 col-md-offset-2 col-md-5 col-lg-offset-2 col-lg-5">
    <%= f.input :destinable_id, collection: Supplier.all, label: 'Proveedor', label_method: :name, value_method: :id, include_blank: 'Elija proveedor...' %>
    <%= f.input :destinable_type, input_html: { value: 'Supplier' }, as: :hidden %>
  </div>
</div>
