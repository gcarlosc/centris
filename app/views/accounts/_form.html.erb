<%= simple_form_for(@account) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
	<%= f.input :organization_name, autofocus: true %>
	<%= f.input :web %>
	<%= f.input :subdomain %>
  </div>
  <div class="form-actions">
	<%= f.button :submit, class: 'btn btn-success' %>
  </div><br>
<% end %>

