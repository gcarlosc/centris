<section class="wrapper">
    <div class="col-xs-12 col-md-12 col-md-12 col-lg-12 size-adaptable">
        <div class="col-xs-12 col-sm-12 col-md-offset-1 col-md-10 col-lg-10">
            <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                    <h4>* Listado de Proyectos</h4>
                </div>
            </div>
        </div>

        <div class="col-xs-12 col-sm-12 col-md-offset-1 col-md-9 col-lg-9">
            <table class="table table-bordered">
                <thead>
                    <tr>
                      <th>Codigo Proyecto</th>
                        <th>Nombre</th>
                        <th>Direccion</th>
                        <th>Organizacion</th>
                    </tr>
                </thead>
                <tbody>
                    <% @projects.each do |project| %>
                    <tr>
                        <td><%= project.id %></td>
                        <td><%= project.name %></td>
                        <td><%= project.address %></td>
                        <td><%= Organization.find(project.organization_id).name %></td>
                    </tr>
                    <% end %>
                </tbody>
            </table>
        </div>
    </div>
</section>
